<div class="not-transparent panel callout radius">
  <h1>Add New Park</h1>

  <%= form_for @park do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :street, "Address" %>
    <%= f.text_field :street %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.submit "Submit" %>
  <% end %>
</div>
