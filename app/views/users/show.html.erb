<div>
  <img src="<%= @user.avatar.url %>" height="300" width="300">
  <h3>
    <%= @user.email %>
  </h3>
</div>
<dl>
  <h4>Your Reviews:</h4>
  <% @reviews.each do |review| %>
    <dt>Park Name: <%= review.park.name %></li>
    <dd>
      <li>Review: <%= review.body %></li>
    </dd>
  </dt>
  <% end %>
</dl>
